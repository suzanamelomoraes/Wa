{
  "name": "wa",
  "version": "0.1.1",
  "description": "React Redux with simple async action",
  "main": "server/index",
  "scripts": {
    "build": "webpack --config client/webpack.config.js",
    "server": "run-s build start",
    "test": "jest --verbose --noStackTrace",
    "test:watch": "npm test -- --watchAll",
    "start": "node server",
    "dev": "run-p dev:server dev:client",
    "dev:server": "nodemon .",
    "dev:client": "webpack --config client/webpack.config.js --watch",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db:reset": "run-s db:delete db:migrate db:seed",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "db:delete": "rm -f server/db/dev.sqlite3",
    "postinstall": "webpack"
  },
  "author": "EDA",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "dependencies": {
    "authenticare": "^0.4.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.3",
    "pg": "^7.14.0",
    "sqlite3": "^4.1.0",
    "superagent": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.3",
    "jest": "^24.8.0",
    "nock": "^10.0.6",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1",
    "supertest": "^4.0.2",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5"
  },
  "jest": {
    "setupFiles": [
      "./tests/client/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
